{
  "name": "maso",
  "version": "1.0.0",
  "description": "Game platform backend for team math competition MaSo.",
  "proxy": "http://localhost:3001/",
  "main": "api/src/cluster.js",
  "scripts": {
    "test": "docker-compose up -d --force-recreate && sleep 5s && NODE_ENV=test node_modules/.bin/mocha --opts api/tests/mocha.opts api/tests/",
    "eslint": "node_modules/.bin/eslint ./",
    "eslint-fix": "node_modules/.bin/eslint ./ --fix",
    "snyk": "snyk auth $SNYK_TOKEN && snyk test",
    "start": "node .",
    "dev": "docker-compose up -d --force-recreate && sleep 4s && NODE_ENV=local node api/scripts/dbimport.js && nodemon ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/snEk42/MaSo.git"
  },
  "keywords": [
    "koa2",
    "team",
    "math",
    "competition"
  ],
  "author": "Jiri Erhart",
  "license": "ISC",
